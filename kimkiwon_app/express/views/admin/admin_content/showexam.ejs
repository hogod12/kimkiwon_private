<!-- Breadcrumbs-->
<ol class="breadcrumb">
  <li class="breadcrumb-item active">시험현황</li>
</ol>

<div class="card mb-3">
  <div class="card-header">
    <i class="fas fa-table"></i>
    시험목록
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>코드</th>
            <th>커리큘럼명</th>
            <th>시험명</th>
            <th>시험타입</th>
            <th>시험분류</th>
            <th>응시인원</th>
            <th>평균</th>
            <th>등록일</th>
            <th>자세히</th>
            <th>수정</th>
            <th>삭제</th>
          </tr>
        </thead>

        <tbody>
          <% for (var i in data['exam_list']){ %>
          <tr>
            <td><%=data['exam_list'][i]["exam_index"]%></td>
            <td><%=data['exam_list'][i]["curriculum_name"]%></td>
            <td><%=data['exam_list'][i]["exam_name"]%></td>
            <td><%=data['exam_list'][i]["exam_type"]%></td>
            <td><%=data['exam_list'][i]["exam_category_1"]%></td>
            <td><%=data['exam_list'][i]["exam_result_people"]%></td>
            <td><%=data['exam_list'][i]["exam_result_avg"]%></td>
            <td><%=data['exam_list'][i]["exam_date"]%></td>
            <td>자세히</td>
            <td>수정</td>
            <td>삭제</td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
</div>