<!-- Breadcrumbs-->
<ol class="breadcrumb">
  <li class="breadcrumb-item active">반 추가</li>
</ol>



<div class="card mb-3">
  <div class="card-header">
    <i class="fas fa-table"></i>
    시험 별 검색
  </div>
  <div class="card-body">
    
    <form action="addclassdata" method="post">
      <div class="form-group">
        <div class="form-label-group">
          <select name="curriculum_index">
            <option value="-1" disabled="disabled" selected="selected" style="width:300px">커리큘럼선택</option>
            <% for (var i in data["curriculum_list"]) { %>
              <option value="<%=data["curriculum_list"][i]["curriculum_index"]%>">[<%=data["curriculum_list"][i]["curriculum_year"]%>] <%=data["curriculum_list"][i]["curriculum_name"]%></option>
            <% } %>
          </select>
        </div>
      </div>

      <div class="form-group">
        <div class="form-label-group">
          <a>스케줄</a>
          <input type="text" id="class_schedule" class="form-control" required="required" name="class_schedule">
        </div>
      </div>

      <div class="form-group">
        <div class="form-label-group">
          <a>수강연도</a>
          <input type="text" id="class_year" class="form-control" required="required" name="class_year">
        </div>
      </div>

      <input type="submit" class="btn btn-primary" value="반 추가">
    </form>
      
  </div>
  <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
</div>
